RewriteEngine On 

RewriteRule ^$ index.php?language=fr&page=1
RewriteRule ^annonces$ index.php?language=fr&page=2&tp=basic
RewriteRule ^ad-(.*)$ index.php?language=fr&page=3&tp=basic&idad=$1
RewriteRule ^contact$ index.php?language=fr&page=4&tp=contact
RewriteRule ^statics$ index.php?language=fr&page=5&tp=premium
RewriteRule ^profil$ index.php?language=fr&page=6&tp=basic
RewriteRule ^pages-fb$ index.php?language=fr&page=7&tp=basic
RewriteRule ^mespages-fb$ index.php?language=fr&page=8&tp=basic
RewriteRule ^deconnexion$ index.php?language=fr&logout
ErrorDocument 404 index.php?language=fr&page=404&logout
